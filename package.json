{
  "name": "policy-dialogue",
  "packageManager": "yarn@3.2.0-rc.1",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development next dev -p 8082",
    "build": "next build",
    "export": "next export",
    "build-analyze": "next build --profile",
    "start": "next start",
    "lint": "next lint && eslint --cache --fix --max-warnings=0",
    "format": "prettier --write ./**/*.{ts,tsx}",
    "type-check": "tsc --pretty --noEmit"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "pretty-quick --staged",
      "eslint --cache --fix"
    ],
    "*.graphql": [
      "prettier --write"
    ]
  },
  "dependencies": {
    "@formspree/react": "^2.2.4",
    "@headlessui/react": "^1.5.0",
    "@heroicons/react": "^1.0.6",
    "firebase": "^9.6.8",
    "graphql": "^16.3.0",
    "graphql-request": "^4.1.0",
    "jotai": "^1.6.1",
    "next": "12.1.0",
    "next-urql": "^3.3.2",
    "react": "17.0.2",
    "react-countup": "^6.1.1",
    "react-dom": "17.0.2",
    "react-google-recaptcha-v3": "^1.9.7",
    "react-hot-toast": "^2.2.0",
    "react-intersection-observer": "^8.33.1",
    "react-slick": "^0.28.1",
    "react-use": "^17.3.2",
    "sharp": "^0.30.3",
    "slick-carousel": "^1.8.1",
    "urql": "^2.2.0"
  },
  "devDependencies": {
    "@tailwindcss/aspect-ratio": "0.4.0",
    "@tailwindcss/forms": "0.5.0",
    "@tailwindcss/line-clamp": "0.3.1",
    "@tailwindcss/typography": "0.5.2",
    "@types/gtag.js": "^0.0.9",
    "@types/prettier": "^2",
    "@types/react": "17.0.40",
    "@types/react-slick": "^0",
    "@types/sharp": "^0",
    "@types/slick-carousel": "^1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "autoprefixer": "10.4.4",
    "eslint": "8.11.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.6",
    "postcss": "8.4.12",
    "prettier": "^2.6.0",
    "pretty-quick": "^3.1.3",
    "tailwindcss": "~3.0.23",
    "tailwindcss-debug-screens": "^2.2.1",
    "typescript": "^4.6.2"
  },
  "engines": {
    "node": ">=14"
  }
